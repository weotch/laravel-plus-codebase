{
    "name": "bkwld/codebasehq",
    "description": "Tools for integrating Laravel apps with CodebaseHQ features",
    "authors": [{ "name": "Robert Reinhard", "email": "info@bkwld.com" }],
    "license": "MIT",
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "^5.0",
        "airbrake/phpbrake": "^1.0@dev"
    },
    "autoload": {
        "psr-4": {
            "Bkwld\\CodebaseHQ\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Bkwld\\Codebase\\ServiceProvider"
            ]
        }
    }
}
